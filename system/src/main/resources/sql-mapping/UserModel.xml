<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.james.system.dao.UserDao">

    <select id="getUsers" resultType="com.james.system.model.UserModel">
        SELECT * FROM USERS
    </select>

    <select id="getUserByUsername" parameterType="java.lang.String" resultType="com.james.system.model.UserModel">
        SELECT * FROM USERS WHERE username = #{username}
    </select>
</mapper>